using System;
class Program
{
    public static void Main(string[] args)
    {
        DateTime data1 = new DateTime(2025, 02, 04);
        DateTime data2 = new DateTime(2004, 07, 01);
        TimeSpan diferença = data1 - data2;

        Console.WriteLine("A diferença entre as duas datas é {0} dias", diferença.Days);

    }
}
