using System;

class Program
{
    public static void Main(string[] args)
    {
        Console.Write("Escreva dois números: ");
        int num1 = int.Parse(Console.ReadLine());
        int num2 = int.Parse(Console.ReadLine());

        Console.WriteLine(num1 % num2 == 0 ? "È divisível." : "Não é divisivel.");

    }
}
