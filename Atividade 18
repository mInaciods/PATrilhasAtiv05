using System;
  class Program
  {
      static void Main()
      {
          Console.Write("\nEscreva um número: ");
          int numero = int.Parse(Console.ReadLine());
          int i = 1;
          
          do {   
            int resultado = numero * i;
            Console.WriteLine("{0} x {1} = {2}", numero, i, resultado);
            i++;
          } while (i <= 10);
      }
  }
